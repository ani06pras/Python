s1="***Python***"
s2="**Python******"
s3="********Java********"
print("Your string is {} and after lstrip method {} ".format(s1,s1.lstrip("*")))
print("Your string is {} and after rstrip method {} ".format(s2,s2.rstrip("*")))
print("Your string is {} and after strip method {} ".format(s3,s3.strip("*")))
